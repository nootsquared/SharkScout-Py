                theme_width: "Custom"
                theme_height: "Custom"
                height: "56dp"
                width: "200dp"
                radius: [5, 5, 5, 5]
                theme_line_color: "Custom"
                line_color: [2/255, 136/255, 209/255, 1]
                theme_bg_color: "Custom"
                md_bg_color: [0.95, 0.95, 0.95, .05]

                on_release: root.export_data()

                MDButtonText:
                    text: "EXPORT DATA"
                    theme_text_color: "Custom"
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    text_color: [1, 1, 1, 1] 
                    theme_font_size: "Custom"
                    theme_font_name: "Custom"
                    font_name: "Roboto"
                    theme_font_size: "Custom"
                    font_size: "16sp"
                    halign: 'center'
                    valign: 'center'

            MDButton:
                style: "outlined"
                pos_hint: {'center_x': 0.5, 'center_y': 0.61}
                theme_width: "Custom"
                theme_height: "Custom"
                height: "56dp"
                width: "200dp"
                radius: [5, 5, 5, 5]
                theme_line_color: "Custom"
                line_color: [2/255, 136/255, 209/255, 1]
