                        text: "Bluetooth"
                        theme_text_color: "Custom"
                        text_color: [1, 1, 1, 1]
                        theme_font_size: "Custom"
                        font_size: "16sp"

                MDSegmentedButtonItem:
                    theme_line_color: "Custom"
                    line_color: [2/255, 136/255, 209/255, 1]
                    selected_color: [2/255, 136/255, 209/255, 1]
